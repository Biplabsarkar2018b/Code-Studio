#include <bits/stdc++.h> 
vector<vector<int>> pairSum(vector<int> &arr, int s){
   vector<vector<int>> ans;
    unordered_map<int,int> m;
    for(int i=0;i<arr.size();i++){
        int val = s-arr[i];
        if(m[val]){
            for(int i=1;i<=m[val];i++){
            vector<int> temp;
            temp.push_back(val);
            temp.push_back(arr[i]); 
            ans.push_back(temp);
            }
        }
        m[arr[i]]++;
    }
          return ans;
}
